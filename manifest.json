{
  "manifest_version": 2,
  "name": "Auto Zoom",
  "description": "Automatic zoom into the main content of a page.",
  "version": "1.1",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "pageAgent.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://*/*"
  ]
}
